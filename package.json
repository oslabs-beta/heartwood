{
  "name": "heartwood",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main/main.js",
  "build": {
    "appId": "io.heartwood.app",
    "directories": {
      "output": "release"
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "scripts": {
    "start": "electron-forge start",
    "electron": "electron .",
    "build": "npm run build:main && npm run build:renderer && npm run build:server",
    "build:renderer": "webpack --config webpack.config.js",
    "build:main": "tsc -p tsconfig.main.json",
    "build:server": "tsc -p tsconfig.server.json",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\" \"npm run dev:server\" \"npm run dev:electron\"",
    "dev:renderer": "cross-env NODE_ENV=development webpack serve --config webpack.config.js",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:server": "tsc -p tsconfig.server.json --watch",
    "dev:electron": "cross-env NODE_ENV=development nodemon --watch dist/main --watch dist/server -e js --exec \"electron .\"",
    "dev:nodemon": "nodemon --watch dist/server -e js --exec \"node dist/server/server.js\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "postinstall": "electron-builder install-app-deps",
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/plugin-transform-runtime": "^7.25.7",
    "@babel/plugin-transform-typescript": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@babel/runtime": "^7.25.7",
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-deb": "^7.5.0",
    "@electron-forge/maker-rpm": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "daisyui": "^4.12.10",
    "electron": "^32.0.1",
    "electron-reloader": "^1.2.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "tailwindcss": "^3.4.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch": "^3.650.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.675.0",
    "@aws-sdk/client-lambda": "^3.675.0",
    "@aws-sdk/client-sts": "^3.637.0",
    "@aws-sdk/credential-providers": "^3.637.0",
    "@reduxjs/toolkit": "^2.2.6",
    "autoprefixer": "^10.4.20",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.4",
    "chartjs-adapter-date-fns": "^3.0.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "electron-oauth2": "^3.0.0",
    "electron-squirrel-startup": "^1.0.1",
    "html-webpack-plugin": "^5.6.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.8.0",
    "mongoose": "^8.6.0",
    "postcss": "^8.4.41",
    "postcss-loader": "^8.1.1",
    "querystring": "^0.2.1",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-chartjs-2": "^5.2.0",
    "react-daisyui": "^5.0.4",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.1",
    "redux": "^5.0.1",
    "simple-oauth2": "^5.1.0",
    "style-loader": "^4.0.0",
    "ts-node": "^10.9.2",
    "webpack": "^5.92.1"
  }
}
